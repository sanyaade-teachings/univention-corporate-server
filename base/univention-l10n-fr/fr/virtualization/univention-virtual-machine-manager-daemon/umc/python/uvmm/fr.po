msgid ""
"The connection to the univention-virtual-machine-manager-daemon service "
"failed."
msgstr ""
"Échec de la connexion au service « univention-virtual-machine-manager-"
"daemon »."

#: umc/python/uvmm/uvmmd.py:68
#, python-format
