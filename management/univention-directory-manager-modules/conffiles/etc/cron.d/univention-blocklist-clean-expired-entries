if configRegistry.get('directory/manager/blocklist/enabled', False) and configRegistry.get('server/role') == 'domaincontroller_master':
    script = "/usr/share/univention-directory-manager-tools/univention-blocklist-clean-expired-entries"
    interval = configRegistry.get("directory/manager/blocklist/cleanup/cron", "0 8 * * *")

    if interval:
        print("%s       root    [ -x %s ] && %s --remove-expired >/dev/null" % (interval, script, script))
@!@